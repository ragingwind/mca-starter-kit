{
  "name": "Chrome Apps Starter Kit",
  "description": "Chrome Apps Starter Kit",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.1.2",
  "offline_enabled": true,
  "icons": {
    "16": "chrome-apps/icons/icon16.png",
    "36": "chrome-apps/icons/icon36.png",
    "48": "chrome-apps/icons/icon48.png",
    "57": "chrome-apps/icons/icon57.png",
    "72": "chrome-apps/icons/icon72.png",
    "96": "chrome-apps/icons/icon96.png",
    "114": "chrome-apps/icons/icon114.png",
    "128": "chrome-apps/icons/icon128.png",
    "144": "chrome-apps/icons/icon144.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "<all_urls>"
  ]
}
